# Supabase (Client)
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Prisma / Database (Supabase Postgres)
# 참고: Supabase 프로젝트의 Project Settings → Database → Connection string (URI)
DATABASE_URL=postgresql://postgres:YOUR_PASSWORD@YOUR_HOST:5432/postgres?pgbouncer=true&connection_limit=1
# 선택: 직접 연결이 필요할 때 사용
DIRECT_URL=postgresql://postgres:YOUR_PASSWORD@YOUR_HOST:6543/postgres


